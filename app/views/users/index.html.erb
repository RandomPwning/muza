
<form class="container" name="loginForm">
  <!--  Login Form -->
  <h4>Login</h4>

  <fieldset class="form-group">
    <input ng-model="" type="email" class="form-control" placeholder="email" title="Email" />
  </fieldset>
 
  <fieldset class="form-group">
    <input ng-model="" type="password" class="form-control" placeholder="password" title="Password" />
  </fieldset>
  
  <fieldset class="form-group">
    <input type="submit" class="btn btn-primary pull-right" value="Submit" />
  </fieldset>

</form>

